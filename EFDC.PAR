C
C
      PARAMETER (NWQVM=22,NWQZM=8,NWQPSM=132,NWQTDM=2000,NWQTSM=32,
     $  NTSWQVM=22,LCMWQ=2560,NSMGM=3,NSMZM=8,NTSSMVM=3,NSMTSM=32,
     $  NWQCSRM=2,NDWQCSR=16384,NWQPSRM=128,NDWQPSR=12288)
C
